{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "target": "ES2022",
    "typeRoots": [
      "./node_modules/@types",
      "./node_modules/@angular",
      "./src/typings.d.ts"
    ],
    "outDir": "./out-tsc/app",
    "types": [
      "node", "material"
    ]
  },
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "src/test.ts",
    "src/**/*.spec.ts"
  ]
}
